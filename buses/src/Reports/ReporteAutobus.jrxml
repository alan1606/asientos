	<queryString>
		<![CDATA[select asiento.numero, cliente.nombre, detalle.viaje, group_concat(" ", hotel.nombre, " " ,d.habitaciones) as habitaciones, detalle.sube, DATE_FORMAT(detalle.hora, "%I:%i %p") as hora, cliente.telefono from asiento 
join cliente on asiento.id_cliente = cliente.id
join viaje on viaje.id = asiento.id_viaje
left join detalle on detalle.id_viaje = viaje.id and cliente.id = detalle.id_cliente
left join detalle_hotel_destino_viaje d on  d.id_detalle = detalle.id
left join hotel_destino_viaje h on h.id = d.id_hotel_destino_viaje
left join hotel on hotel.id = h.id_hotel
left join destino on destino.id = h.id_destino
where asiento.id_viaje = $P{idViaje}
group by (asiento.id)
order by asiento.numero;]]>
	</queryString>
